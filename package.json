{
  "name": "@rednax/monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lerna": "lerna",
    "build": "lerna exec node ../../tools/tasks/build && lerna exec cp ../../README.md dist/README.md",
    "watch": "cross-env WATCH_MODE=true lerna exec --parallel node ../../tools/tasks/build",
    "build-tools": "tsc -p ./tools/tsconfig.json",
    "clean-all": "lerna clean && rimraf libs/*/dist",
    "clean": "rimraf libs/*/dist",
    "test": "jest",
    "test-tdd": "jest --watchAll --no-cache",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "release": "yarn lerna version && yarn build && yarn lerna publish from-git",
    "postinstall": "npm run build-tools",
    "lint": "tslint libs/**/*.ts -t verbose",
    "aio": "compodoc -p ./tsconfig.json --output ./aio --hideGenerator --silent"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.0",
    "@angular/common": "^10.0.0",
    "@angular/compiler": "^10.0.0",
    "@angular/core": "^10.0.0",
    "@angular/forms": "^10.0.0",
    "@angular/platform-browser": "^10.0.0",
    "@angular/platform-browser-dynamic": "^10.0.0",
    "@angular/router": "^10.0.0",
    "@nrwl/angular": "10.2.0",
    "@types/zone.js": "^0.5.12",
    "dnd-core": "^11.1.3",
    "dnd-multi-backend": "^6.0.0",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "rxjs": "~6.6.3",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "~10.1.2",
    "@nrwl/workspace": "10.2.0",
    "@types/node": "~14.11.1",
    "dotenv": "8.2.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "eslint": "7.9.0",
    "typescript": "~4.0.2",
    "prettier": "2.1.2",
    "@angular/compiler-cli": "^10.0.0",
    "@angular/language-service": "^10.0.0",
    "@angular-devkit/build-angular": "~0.1001.2",
    "codelyzer": "~6.0.0",
    "jest-preset-angular": "8.3.1",
    "@nrwl/jest": "10.2.0",
    "jest": "26.4.2",
    "@types/jest": "26.0.14",
    "ts-jest": "26.3.0",
    "cypress": "^5.2.0",
    "@nrwl/cypress": "10.2.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@compodoc/compodoc": "^1.1.9",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.87",
    "chokidar-cli": "^2.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "fs-extra": "^9.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "jest-localstorage-mock": "^2.2.0",
    "lerna": "^3.2.1",
    "lodash": "^4.17.4",
    "mermaid.cli": "^0.5.1",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.0",
    "scss-bundle": "^3.0.1",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.1"
  }
